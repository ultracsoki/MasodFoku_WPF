        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="tbA" HorizontalAlignment="Left" Margin="191,111,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="43" Height="41" FontSize="20"/>
        <TextBox x:Name="tbB" HorizontalAlignment="Left" Margin="289,111,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="43" Height="41" FontSize="20"/>
        <TextBox x:Name="tbC" HorizontalAlignment="Left" Margin="363,111,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="43" Height="41" FontSize="20"/>
