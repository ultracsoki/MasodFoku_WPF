        <Button x:Name="buttonSzamol" Content="Számol" HorizontalAlignment="Left" Margin="191,175,0,0" VerticalAlignment="Top" FontSize="20" Height="42" Width="98" Click="buttonSzamol_Click"/>

    </Grid>
</Window>
