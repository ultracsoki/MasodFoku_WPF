        <Label Content="x+" HorizontalAlignment="Left" Margin="332,111,0,0" VerticalAlignment="Top" Height="41" Width="41" FontSize="20"/>
        <Label Content="=0" HorizontalAlignment="Left" Margin="403,111,0,0" VerticalAlignment="Top" Height="41" Width="42" FontSize="20"/>
        <Label x:Name="labelEredmeny1" Content="x1 =" HorizontalAlignment="Left" Margin="191,240,0,0" VerticalAlignment="Top" Height="28" Width="98"/>
